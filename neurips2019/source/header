    <script>

          var url = window.location.href;
          var page = url.split(/[\\/]/).pop();

          $.each($(".blog-nav a"), function(i, e) {
              var p = $(e).attr("href");

              var s1 = page.split("_")[0];
              var s2 = p.split("_")[0];

              if (p==page | s1==s2) {
                if ($(e).hasClass('blog-nav-item')) {
                  $(e).addClass('active');
                }
                else {
                  var id = $(e).attr('id');
                  id = id.split('_')[0];
                  $("#"+id).addClass('active');
                }
              }

          });


    </script>

